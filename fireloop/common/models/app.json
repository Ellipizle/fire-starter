{
  "name": "App",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for App registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "id": {
      "name": "id",
      "type": "string"
    },
    "name": {
      "name": "name",
      "type": "string"
    },
    "organizationId": {
      "name": "organizationId",
      "type": "string"
    },
    "createdAt": {
      "name": "createdAt",
      "type": "Date"
    },
    "updatedAt": {
      "name": "updatedAt",
      "type": "Date"
    },
    "userId": {
      "name": "userId",
      "type": "any"
    }
  },
  "validations": [],
  "relations": {
    "organization": {
      "name": "organization",
      "type": "belongsTo",
      "model": "Organization"
    },
    "user": {
      "name": "user",
      "type": "belongsTo",
      "model": "User"
    }
  },
  "acls": [],
  "methods": {}
}
